package helloapp;

import java.util.Scanner;

public class Name {
	public static void main(String[] args){
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
        int d = sc.nextInt();
        
        if ((((a - c) == (b - d)) || ((a - c) == (d - b))) || (((c - a) == (b - d)) || ((a - c) == (d - b))) || ((a == c) && (b == d))) {
             System.out.println("YES");
        }else{
             System.out.println("NO");
        }
        }


        
    }
