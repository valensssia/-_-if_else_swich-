package helloapp;

import java.util.Scanner;

public class Name {
	public static void main(String[] args){
		Scanner sc = new Scanner(System.in);
		int a1 = sc.nextInt();
		int a2 = sc.nextInt();
		int b1 = sc.nextInt();
		int b2 = sc.nextInt();
		if ((a1 == b1) || (a2 == b2)){
			System.out.println("Yes");
			}else{
				System.out.println("No");
				}
			}
	}
