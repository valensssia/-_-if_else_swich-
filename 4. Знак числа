package helloapp;

import java.lang.Math;
import java.util.Scanner;

public class Name {
	public static void main(String[] args){
		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		if (x > 0) {
			System.out.println("1");
		}else {
			if(x == 0) {
				System.out.println("0");
			}else {
				System.out.println("-1");
			}
			}
		}
		}
